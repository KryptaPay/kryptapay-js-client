{
    "name": "kryptapay-js-client",
    "version": "0.4.0",
    "description": "A JavaScript client that allows the abstraction of https requests, management of sessions in a RN Expo application.",
    "scripts": {
        "test": "jest --config jestconfig.json",
        "dev": "rollup -c -w",
        "build": "rollup -c",
        "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
        "docs": "typedoc src --out docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kryptapay/kryptapay-js-client.git"
    },
    "keywords": [
        "https",
        "expo",
        "session"
    ],
    "main": "./dist/index.esm.js",
    "module": "./dist/index.esm.js",
    "files": [
        "dist"
    ],
    "types": "./dist/index.d.ts",
    "type": "module",
    "author": "cedrick-ah",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/kryptapay/kryptapay-js-client/issues"
    },
    "homepage": "https://github.com/kryptapay/kryptapay-js-client#readme",
    "devDependencies": {
        "@microsoft/api-extractor": "^7.38.5",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "eslint": "^8.55.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.1",
        "rollup": "^4.6.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typedoc": "^0.25.4",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "expo-file-system": "^15.6.0",
        "expo-secure-store": "^12.5.0",
        "flatted": "^3.2.7"
    }
}
